program 4

#include <iostream>
#include <string>
using namespace std;

class base{
private:
	int i=9;
public:

	void printNum(){
		cout<<"i = "<<i<<endl;
	}
};


class derive:public base{
private:
	int j=5;
public:
	void printNum(){
		cout<<"j = "<<j<<endl;
	}
};


int main() {
	derive d;
	d.printNum();
	return 0;
	
}
